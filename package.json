{
  "name": "genesis-kit",
  "version": "1.0.0",
  "description": "为算法竞赛(OI/ACM)设计的, 简单易用的测试数据生成器与对拍器。",
  "keywords": [
    "oi",
    "acm",
    "icpc",
    "algorithm",
    "contest",
    "test-data",
    "generator",
    "checker",
    "对拍",
    "genesis-kit"
  ],
  "author": "YV",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yviscool/Genesis/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yviscool/Genesis"
  },
  "module": "./dist/index.mjs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "bun": "./dist/bun/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "test": "bun test",
    "dev": "bun --watch run examples/make.ts",
    "build": "bun run build.ts",
    "release": "bun run build && bun run test && bun publish"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsup": "^8.5.0",
    "esbuild-fix-imports-plugin": "latest"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "consola": "^3.4.2",
    "es-toolkit": "^1.40.0",
    "execa": "^9.6.0",
    "ora": "^9.0.0",
    "picocolors": "^1.1.1"
  }
}